{
  "name": "skynet-drive-memory-mcp",
  "version": "1.0.0",
  "description": "MCP Server pour mémoire Google Drive + RAG sémantique",
  "main": "dist/index.js",
  "type": "module",
  "bin": {
    "skynet-drive-memory-mcp": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc && chmod +x dist/index.js",
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "test": "vitest",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "google-drive",
    "rag",
    "embeddings",
    "memory",
    "claude-code"
  ],
  "author": "Skynet",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.4",
    "@xenova/transformers": "^2.19.0",
    "googleapis": "^140.0.0",
    "winston": "^3.18.0",
    "zod": "^3.25.1"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "@typescript-eslint/eslint-plugin": "^8.18.2",
    "@typescript-eslint/parser": "^8.18.2",
    "eslint": "^9.17.0",
    "prettier": "^3.4.2",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
