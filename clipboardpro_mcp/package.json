{
  "name": "clipboardpro-mcp",
  "version": "1.0.0",
  "description": "Skynet Clipboard Intelligence - AI-powered clipboard manager with OCR and multi-device sync",
  "main": "app/api/server.js",
  "scripts": {
    "start:api": "node app/api/server.js",
    "start:mcp": "node app/mcp/server.js",
    "start:mcp:stdio": "MCP_MODE=stdio node app/mcp/server.js",
    "start:all": "concurrently \"npm run start:api\" \"npm run start:mcp\"",
    "dev": "NODE_ENV=development npm run start:all",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "clipboard",
    "mcp",
    "ai",
    "ocr",
    "sync",
    "skynet",
    "winui3"
  ],
  "author": "Skynet Depot Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/skynet/clipboardpro_mcp.git"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "sqlite3": "^5.1.6",
    "axios": "^1.6.2",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
