{
  "name": "skynet-mcp-workspace",
  "version": "1.0.0",
  "description": "MCP Server complet pour environnement DevOps + Graphics - Workspace AI pour Claude Code",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "skynet-mcp": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "start": "node dist/index.js",
    "inspector": "npx @modelcontextprotocol/inspector node dist/index.js",
    "test": "vitest",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "claude",
    "devops",
    "docker",
    "automation",
    "graphics",
    "ai-tools"
  ],
  "author": "Skynet Project",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.4",
    "zod": "^3.23.8",
    "dockerode": "^4.0.2",
    "execa": "^9.5.2",
    "fs-extra": "^11.2.0",
    "simple-git": "^3.27.0",
    "systeminformation": "^5.23.5",
    "sharp": "^0.33.5",
    "chokidar": "^4.0.3",
    "yaml": "^2.6.1"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "@types/fs-extra": "^11.0.4",
    "@types/dockerode": "^3.3.31",
    "@typescript-eslint/eslint-plugin": "^8.18.2",
    "@typescript-eslint/parser": "^8.18.2",
    "eslint": "^9.17.0",
    "prettier": "^3.4.2",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
